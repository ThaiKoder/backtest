<Window x:Class="backtest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:backtest"
        xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <oxy:PlotView Model="{Binding Chart.Model}" />
        <Button Content="S15" HorizontalAlignment="Left" Margin="57,10,0,0" VerticalAlignment="Top" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand"/>
        <Button Content="M1" HorizontalAlignment="Left" Margin="84,10,0,0" VerticalAlignment="Top" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand" Click="M1_Click"/>
        <Button x:Name="M5" Content="M5" HorizontalAlignment="Left" Margin="111,10,0,0" VerticalAlignment="Top" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand" Click="M5_Click"/>
        <Button Content="M15" HorizontalAlignment="Left" Margin="137,10,0,0" VerticalAlignment="Top" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand" Click="M15_Click"/>
        <Button Content="M30" HorizontalAlignment="Left" Margin="170,10,0,0" VerticalAlignment="Top" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand" Click="M30_Click"/>
        <Button Content="H1" HorizontalAlignment="Left" Margin="203,10,0,0" VerticalAlignment="Top" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand" Click="H1_Click"/>
        <Button Content="H4" HorizontalAlignment="Left" Margin="227,10,0,0" VerticalAlignment="Top" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand" Click="H4_Click"/>
        <Button Content="Backtest" HorizontalAlignment="Right" Margin="0,5,10,0" VerticalAlignment="Top" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand" Click="backtest_Click"/>
        <Button Content="ETH" HorizontalAlignment="Right" Margin="0,0,40,24" VerticalAlignment="Bottom" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand" Click="ETH_Click"/>
        <Button Content="RTH" HorizontalAlignment="Right" Margin="0,0,10,24" VerticalAlignment="Bottom" Background="#FF191919" Foreground="#FFB6B6B6" BorderBrush="#00707070" Cursor="Hand" Click="RTH_Click"/>
    </Grid>
</Window>
